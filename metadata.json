  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.6.0 < 9.0.0" }
  ],
  "requirements": [ { "name": "puppet", "version_requirement": ">= 6.0.0 < 8.0.0" }
